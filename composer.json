{
    "name": "t0mmy742/api-auth",
    "type": "library",
    "description": "A PSR-15 middleware to authenticate API requests. Parts to generate token are already provided.",
    "keywords": [
        "authentication",
        "API",
        "PSR-15",
        "middleware",
        "token"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas LE BERRE",
            "email": "29512640+t0mmy742@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "infection/infection": "^0.25.3",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.99",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "phpstan/phpstan-phpunit": "^0.12.22",
        "phpstan/phpstan-strict-rules": "^0.12.11",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "thecodingmachine/phpstan-strict-rules": "^0.12.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "T0mmy742\\ApiAuthentication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "T0mmy742\\ApiAuthentication\\Tests\\": "tests/"
        }
    }
}
